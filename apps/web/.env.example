# Gateway base URL used for all domain APIs
VITE_GATEWAY_URL=http://localhost:8080

# Optional overrides for direct services (fallback to gateway when unset)
VITE_CRM_URL=http://localhost:8081
VITE_WMS_URL=http://localhost:8082

# Basic auth credentials forwarded to the gateway when required
VITE_GATEWAY_BASIC_AUTH=admin@asfp.pro:admin123

# Enable MSW during development if you want to stub API calls
VITE_ENABLE_MSW=false
